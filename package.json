{
  "name": "webrdfs-client",
  "version": "0.0.1",
  "description": "Web client for webRDFS",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "webpack",
    "build-watch": "npm run build -- --watch",
    "start": "webpack-dev-server --content-base dev/ --watch",
    "lint": "eslint .",
    "test": "jest --verbose test",
    "test-watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/comp413-2017/webrdfs-client.git"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "humanize": "^0.0.9",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-elemental": "^0.0.7",
    "react-elemental-fonts": "^0.0.1",
    "react-helmet": "^5.2.0",
    "time-func": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-disable": "^0.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "html-loader": "^0.5.1",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.1",
    "nodemon": "^1.12.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
